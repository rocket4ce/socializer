<%= current_user.name %>
<%= simple_form_for ([@user, @portafolio]) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :titulo %>
  </div>

  <div class="form-inputs">
    <%= f.input :descripcion %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
